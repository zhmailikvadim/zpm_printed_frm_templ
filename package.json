{
  "name": "zpm_printed_frm_templ",
  "version": "0.0.1",
  "private": true,
  "description": "Цифровое ТОРО. Шаблоны печатных форм",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^3.0.0",
    "@sap/ux-ui5-tooling": "1",
    "@sap/ux-specification": "UI5-1.71",
    "@sap-ux/ui5-middleware-fe-mockserver": "2"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-client=300&sap-language=RU&sap-ui-xx-viewCache=false#zpmprintedfrmtempl-tile\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-client=300&sap-ui-xx-viewCache=false#zpmprintedfrmtempl-tile\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-client=300&sap-ui-xx-viewCache=false\"",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-client=300&sap-ui-xx-viewCache=false#zpmprintedfrmtempl-tile\"",
    "start-variants-management": "fiori run --open \"/preview.html#zpmprintedfrmtempl-tile\""
  },
  "sapux": true,
  "sapuxLayer": "CUSTOMER_BASE"
}
